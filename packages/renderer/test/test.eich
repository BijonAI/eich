<eich width="2000" height="400">
  <var key="c" $value="'U ARE THE MOST FOOLISH PERSON IN THE WORLD'"/>
  <row>
    <for $in="Array(c.length).keys()" key="x">
      <col $grow="x * 2">
        <value $data="x"/>
      </col>
    </for>
  </row>
  <row>
    <for $in="Array(c.length).keys()" key="x">
      <col $grow="x * 2">
        <value $data="c[x]"/>
      </col>
    </for>
  </row>
</eich>
