<eich width="100%" height="100%">
  <var key="c" $value="'U ARE THE MOST FOOLISH PERSON IN THE WORLD'"/>
  <row>
    <for $in="Array(c.length).keys()" key="x">
      <col $grow="x * 2">
        <if $condition="Boolean(x % 2)">
          <value $data="x"/>
        </if>
      </col>
    </for>
  </row>
</eich>
