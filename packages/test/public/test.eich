<eich>
  <container>
    <for key="x" $in="Array(10).keys()">
      <col>
        <for key="y" $in="Array(9).keys()">
          <row $color="`rgb(${x.value * 10},${y.value * 10},${x.value * y.value * 10})`" $transform="`rotate(${x.value * 10}deg) scale(${x.value/3},${y.value/3})`">
            <value $data="x" />
          </row>
        </for>
      </col>
    </for>
  </container>
</eich>